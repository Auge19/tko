{
  "name": "ko",
  "description": "TKO is an experimental fork of Knockout",
  "homepage": "http://knockoutjs.com/",
  "version": "4.0.0-alpha2",
  "license": "MIT",
  "author": "The Knockout.js Team",
  "main": "dist/knockout-latest.debug.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/knockout/knockout.git"
  },
  "scripts": {
    "test": "lerna exec --concurrency=1 -- karma start ../../karma.conf.js --once",
    "lint": "standard",
    "lerna": "lerna bootstrap --hoist --npm-client yarn",
    "build": "npm run build.es6; npm run build.js",
    "build.es6": "rollup -c rollup.config.js -o dist/ko.es6.js",
    "build.js": "USE_BABEL=1 rollup -c rollup.config.js -o dist/ko.js",
    "minify": "npm run minify.es6; npm run minify.js",
    "minify.es6": "MINIFY=1 rollup -c rollup.config.js -o dist/ko.es6.min.js",
    "minify.js": "MINIFY=1 USE_BABEL=1 rollup -c rollup.config.js -o dist/ko.min.js"
  },
  "files": [
    "dist/"
  ],
  "bugs": "https://github.com/knockout/knockout/issues",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "standard": {
    "globals": [
      "it",
      "iit",
      "ddescribe",
      "describe",
      "jasmine",
      "afterEach",
      "beforeEach",
      "expect",
      "testNode",
      "waits",
      "runs"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-proto-to-assign": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "jquery": "^3.2.1",
    "js-yaml": "^3.8.4",
    "karma": "^1.7.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-jasmine": "^0.1.0",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-rollup-plugin": "^0.2.4",
    "karma-sinon": "^1.0.5",
    "lerna": "^2.0.0-rc.4",
    "lodash": "^4.17.4",
    "mocha": "^3.3.0",
    "phantomjs-prebuilt": "^2.1.14",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^3.0.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-stub": "^1.2.0",
    "rollup-plugin-visualizer": "^0.2.1",
    "sinon": "^2.2.0",
    "standard": "^10.0.2"
  },
  "dependencies": {}
}
