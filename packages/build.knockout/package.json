{
  "name": "@tko/build.knockout",
  "description": "Knockout makes it easier to create rich, responsive UIs with JavaScript",
  "homepage": "http://knockoutjs.com/",
  "version": "4.0.0-alpha6.2",
  "license": "MIT",
  "author": "The Knockout.js team",
  "main": "dist/build.knockout.js",
  "scripts": {
    "test": "karma start ../../karma.conf.js --once",
    "build": "rollup -c ../../rollup.config.js",
    "watch": "karma start ../../karma.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knockout/knockout.git"
  },
  "bugs": "https://github.com/knockout/knockout/issues",
  "testling": {
    "preprocess": "build/build.sh",
    "html": "spec/runner.html?src=build/output/knockout-latest.js&testling=true",
    "browsers": [
      "ie/6..latest",
      "chrome/20..latest",
      "firefox/3..latest",
      "safari/5.0.5..latest",
      "opera/11.0..latest",
      "iphone/6..latest",
      "ipad/6..latest"
    ]
  },
  "files": [
    "dist/"
  ],
  "karma": {
    "frameworks": [
      "jasmine"
    ],
    "files": [
      "dist/knockout.es6.js",
      "helpers/jasmine.extensions.js",
      "spec/**/*.js"
    ]
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "devDependencies": {
    "@tko/binding.component": "^4.0.0-alpha6.2",
    "@tko/binding.core": "^4.0.0-alpha6.2",
    "@tko/binding.foreach": "^4.0.0-alpha6.2",
    "@tko/binding.if": "^4.0.0-alpha6.2",
    "@tko/binding.template": "^4.0.0-alpha6.2",
    "@tko/builder": "^4.0.0-alpha6.2",
    "@tko/filter.punches": "^4.0.0-alpha6.2",
    "@tko/provider.attr": "^4.0.0-alpha6.2",
    "@tko/provider.bindingstring": "^4.0.0-alpha6.2",
    "@tko/provider.component": "^4.0.0-alpha6.2",
    "@tko/provider.databind": "^4.0.0-alpha6.2",
    "@tko/provider.multi": "^4.0.0-alpha6.2",
    "@tko/provider.mustache": "^4.0.0-alpha6.2",
    "@tko/provider.virtual": "^4.0.0-alpha6.2",
    "@tko/utils.component": "^4.0.0-alpha6.2",
    "@tko/utils.functionrewrite": "^4.0.0-alpha6.2",
    "tslib": "^1.8.0"
  },
  "__about__shared.package.json": "These properties are copied into all packages/*/package.json.  Run `yarn repackage`",
  "standard": {
    "env": [
      "browser",
      "jasmine",
      "mocha"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
